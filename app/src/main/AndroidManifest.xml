<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flori.swim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cards"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".startScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".playerSelect" />
        <activity android:name=".askForNames" />
        <activity android:name=".insertName" />
        <activity android:name=".lifeSelect" />
        <activity android:name=".mainScreen" />
        <activity android:name=".settings" />
        <activity
            android:name=".newGamePopUp"
            android:theme="@style/AppTheme.popStyle" />
        <activity
            android:name=".winningPopUp"
            android:theme="@style/AppTheme.popStyle" />
        <activity android:name=".anleitung"></activity>
    </application>

</manifest>